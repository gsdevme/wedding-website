{% if navigation_li_class is not defined %}
    {% set navigation_li_class = '' %}
{% endif %}

<div class="nav-container">
    <nav class="nav nav-1 {% block navigation_class %}transparent light{% endblock %}">
        <a href="/" class="logo">
            <span>Ceilidh &amp; Gavin</span>
        </a>
        <ul class="menu">
            <li><a href="{{ url('home') }}#home" class="{{ navigation_li_class }}">home</a></li>
            <li><a href="{{ url('home') }}#wedding" class="{{ navigation_li_class }}">wedding</a></li>
            <li><a href="{{ url('home') }}#location" class="{{ navigation_li_class }}">location</a></li>
            {#            <li><a href="#registry" class="inner-link" target="default">gifts</a></li>#}
            <li><a href="{{ url('home') }}#rsvp" class="{{ navigation_li_class }}">rsvp</a></li>
        </ul>
        <div class="mobile-toggle">
            <div class="line-1"></div>
            <div class="line-2"></div>
            <div class="line-3"></div>
        </div>
    </nav>

</div>

<div class="main-container">
    {% block main %}{% endblock %}
</div>

